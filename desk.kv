# File name: menubar.kv
#:kivy 1.9.0
#:import desk desk

<NoteDesk@Desk>
	filechooser: _filechooser
	FileChooserIconView:
        id: _filechooser
		path: 'note'
		rootpath: 'note'
        on_selection: root.selected(_filechooser.selection)
		on_entry_added: root.file_count(_filechooser.path)
		
<PictureDesk@Desk>
	filechooser: _filechooser
	FileChooserIconView:
        id: _filechooser
		path: 'picture'
		rootpath: 'picture'
        on_selection: root.selected(_filechooser.selection)
		on_entry_added: root.file_count(_filechooser.path)